{
  "compilerOptions": {
    "outDir": "dist",
  "module": "ESNext",
  "target": "ES2022",
  "moduleResolution": "node",
  "types": ["node"],
  "resolveJsonModule": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true
  },
  "include": [
  "src/**/*"
  ]
}